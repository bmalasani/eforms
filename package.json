{
  "name": "@meb/eforms",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "app",
    "api",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "simple-git-hooks": "^2.8.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "packageManager": "npm@8.19.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmalasani/eforms.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmalasani/eforms/issues"
  },
  "homepage": "https://github.com/bmalasani/eforms#readme"
